{
    "compilerOptions": {
        "experimentalDecorators": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "module": "esnext",
        "target": "es2019",
        "lib": [
          "esnext",
          "dom",
          "webworker"
        ],
        "declaration": false,
        "declarationMap": false,
        "baseUrl": ".",
        "paths": {},
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true
    },
    "include": [
      "src/**/*"
    ]
}
